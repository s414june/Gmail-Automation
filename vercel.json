{
	"version": 2,
	"builds": [
		{
			"src": "server.ts",
			"use": "@vercel/node"
		},
		{
			"src": "dist/**/*",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "dist/server.js"
		},
		{
			"src": "/(.*)",
			"dest": "/dist/$1"
		}
	],
	"buildCommand": "npm install && npm run build"
}
